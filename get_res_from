if [ -n "$1" ]; then
server=$1
time=`date +%F-%T | sed 's/[:|-]//g'`
yesterday=`date -d yesterday +%F| sed 's/-//g'`
filename=~/$server.daily-$yesterday.$time.cpuload.csv
#echo "$time $server $filename $yesterday"
scp xcast@$server:/usr/local/registrator/lib/mserver/app/cpuload.php.$yesterday.res $filename && \
sleep 3; /usr/bin/php -e  /home/nir/bin/convert_res_from_pbx.php -i $filename
fi
